<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Linkedin">

   <test name="Login test Chrome">
        <parameter name="browserType" value="Chrome"/>
        <parameter name="envURL" value="https://www.linkedin.com/" />
        <parameter name="envURL" value="https://ua.linkedin.com/" />
        <parameter name="envURL" value="https://ru.linkedin.com/" />
        <classes>
            <class name="test.LinkedinLoginTest" />
        </classes>
   </test>

   <test name="Login test FireFox">
        <parameter name="browserType" value="FireFox"/>
        <parameter name="envURL" value="https://www.linkedin.com/" />
        <parameter name="envURL" value="https://ua.linkedin.com/" />
        <parameter name="envURL" value="https://ru.linkedin.com/" />
        <classes>
            <class name="test.LinkedinLoginTest" />
        </classes>
   </test>


    <test name="Login test ie">
        <parameter name="browserType" value="ie"/>
        <parameter name="envURL" value="https://www.linkedin.com/" />
        <parameter name="envURL" value="https://ua.linkedin.com/" />
        <parameter name="envURL" value="https://ru.linkedin.com/" />
        <classes>
            <class name="test.LinkedinLoginTest" />
        </classes>
    </test>

</suite>

